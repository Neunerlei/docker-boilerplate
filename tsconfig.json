{
  "include": [
    "./builder/**/*.ts",
    "./partials/**/*.ts",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "typeRoots": [
      "./node_modules/@types",
      "./out/bin/_env/node_modules/@types",
    ],
    "paths": {
      "@builder/*": ["./builder/*"],
      "@/*": ["./partials/root/files/bin/_env/core/*"],
      "commander": ["./out/bin/_env/node_modules/commander"],
      "glob": ["./out/bin/_env/node_modules/glob"],
      "chalk": ["./out/bin/_env/node_modules/chalk"],
    },
    "module": "esnext",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "target": "esnext",
    "allowImportingTsExtensions": true
  }
}
